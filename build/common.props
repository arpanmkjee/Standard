<Project>

	<Import Project="version.props" />

	<PropertyGroup Label="Build">
		<TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
	</PropertyGroup>

	<PropertyGroup Label="Package">
		<Product>Cosmos.Standard</Product>
		<Authors>AlexLEWIS;dotnetcore;StarrySky;Cosmos</Authors>
		<RepositoryUrl>https://github.com/cosmos-loops/Standard</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageProjectUrl>https://github.com/cosmos-loops/Standard</PackageProjectUrl>
		<PackageIcon>icon.png</PackageIcon>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageTags>StarrySky;Cosmos;Standard;Framework</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\LICENSE" Pack="true" Visible="false" PackagePath=""/>
		<None Include="..\..\msic\images\icon.png" Pack="true" PackagePath=""/>
	</ItemGroup>

</Project>